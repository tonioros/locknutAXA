<% include ../Shared/layout%>
<div class="row" ng-controller="servicioCTRL">
    <input type="text" class="form-control" placeholder="Buscar..." ng-model="searchSER" style="width: 60%">
    <div class="col-lg-12">
        <div ng-repeat="servi in servicioList | filter: searchSER" style="border-radius: 4px; background: #e3dddd; border-bottom: solid 1px #282828; padding: 1.5% 3%; margin: 5px">
            <h4>Hecho por: {{servi.nombreUsuario}} - ( {{servi.codigoUsuario}} )</h4>
            <h5>Realizado en la fecha: {{servi.fechaServicio}}</h5>
            <h5>Automovil: {{servi.marca+" "+servi.modelo+" "+servi.anio+" ("+servi.codigoAuto+") " }}</h5>
        </div>
    </div>
</div>
<% include ../Shared/footer%>